  "agents": {
    "defaults": {
      "workspace": "/Users/username/clawd",
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 2,
      "subagents": {
        "maxConcurrent": 2
      }
    },
    "list": [
      {
        "id": "main"
      },
      {
        "id": "trading-bot",
        "name": "trading-bot",
        "workspace": "/Users/username/repos/trading-bot",
        "agentDir": "/Users/username/.clawdbot/agents/trading-bot/agent"
      },
      {
        "id": "game-server",
        "name": "game-server",
        "workspace": "/Users/username/repos/game-server",
        "agentDir": "/Users/username/.clawdbot/agents/game-server/agent"
      }
    ]
  },
  "messages": {
    "ackReactionScope": "all"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "telegram": {
      "dmPolicy": "pairing",
      "botToken": "YOUR_BOT_TOKEN_FROM_BOTFATHER",
      "groups": {
        "*": {
          "requireMention": true
        }
      },
      "groupPolicy": "allowlist",
      "streamMode": "partial",
      "reactionNotifications": "own",
      "reactionLevel": "minimal"
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "lan",
    "auth": {
      "token": "YOUR_GATEWAY_AUTH_TOKEN"
    }
  },
  "plugins": {
    "entries": {
      "google-antigravity-auth": {
        "enabled": true
      },
      "telegram": {
        "enabled": true
      }
    }
