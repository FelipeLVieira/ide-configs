{
	"servers": {
		"github/github-mcp-server": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/",
			"gallery": "https://api.mcp.github.com",
			"version": "0.13.0"
		},
		"microsoft/playwright-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.0.1-seed"
		},
		"microsoft/markitdown": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"markitdown-mcp==0.0.1a4"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"upstash/context7": {
			"type": "http",
			"url": "https://mcp.context7.com/mcp",
			"headers": {
				"CONTEXT7_API_KEY": "${input:context7_api_key}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"coplaydev/unity-mcp": {
			"type": "stdio",
			"command": "uv",
			"args": [
				"--directory",
				"${input:unity_mcp_server_src}",
				"run",
				"server.py"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"chromedevtools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@latest",
				"--browserUrl",
				"${input:browser_url}",
				"--headless",
				"${input:headless}",
				"--isolated",
				"${input:isolated}",
				"--channel",
				"${input:chrome_channel}"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.0.1-seed"
		},
		"firecrawl/firecrawl-mcp-server": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"firecrawl-mcp@latest"
			],
			"env": {
				"FIRECRAWL_API_KEY": "${input:api_key}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"oraios/serena": {
			"type": "stdio",
			"command": "uvx",
			"args": [
				"--from",
				"git+https://github.com/oraios/serena",
				"serena",
				"start-mcp-server",
				"serena==latest",
				"--context",
				"ide-assistant"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"io.github.upstash/context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@upstash/context7-mcp@1.0.25"
			],
			"env": {
				"CONTEXT7_API_KEY": "${input:CONTEXT7_API_KEY}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "1.0.0"
		},
		"io.github.github/github-mcp-server": {
			"type": "stdio",
			"command": "docker",
			"args": [
				"run",
				"-i",
				"--rm",
				"run",
				"-i",
				"true",
				"--rm",
				"true",
				"-e",
				"GITHUB_PERSONAL_ACCESS_TOKEN=${input:token}",
				"ghcr.io/github/github-mcp-server",
				"ghcr.io/github/github-mcp-server:0.21.0"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.21.0"
		},
		"io.github.ChromeDevTools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@0.10.1"
			],
			"gallery": "https://api.mcp.github.com",
			"version": "0.10.1"
		},
		"com.supabase/mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"--project-ref",
				"${input:project-ref}",
				"--read-only",
				"${input:read-only}",
				"--features",
				"${input:features}",
				"--api-url",
				"${input:api-url}",
				"@supabase/mcp-server-supabase@0.5.9"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "${input:SUPABASE_ACCESS_TOKEN}"
			},
			"gallery": "https://api.mcp.github.com",
			"version": "0.5.9"
		},
		"com.stripe/mcp": {
			"type": "http",
			"url": "https://mcp.stripe.com",
			"gallery": "https://api.mcp.github.com",
			"version": "0.2.4"
		}
	},
	"inputs": [
		{
			"id": "context7_api_key",
			"type": "promptString",
			"description": "Context7 API key (optional; increases rate limits). Get one at https://context7.com/dashboard",
			"password": true
		},
		{
			"id": "unity_mcp_server_src",
			"type": "promptString",
			"description": "Absolute path to the Unity MCP Server 'src' folder installed by the Unity package (e.g., macOS: ~/Library/Application Support/UnityMCP/UnityMcpServer/src; Windows: %USERPROFILE%/AppData/Local/UnityMCP/UnityMcpServer/src; Linux: ~/.local/share/UnityMCP/UnityMcpServer/src).",
			"password": false
		},
		{
			"id": "browser_url",
			"type": "promptString",
			"description": "Optional: connect to an already-running Chrome (remote debugging / port-forward). Example: http://127.0.0.1:9222",
			"password": false
		},
		{
			"id": "headless",
			"type": "promptString",
			"description": "Run Chrome headless (true/false). Default: false",
			"password": false,
			"default": "false"
		},
		{
			"id": "isolated",
			"type": "promptString",
			"description": "Use a temporary user-data-dir (true/false). Default: false",
			"password": false,
			"default": "false"
		},
		{
			"id": "chrome_channel",
			"type": "promptString",
			"description": "Chrome channel: stable | canary | beta | dev (default: stable).",
			"password": false,
			"default": "stable"
		},
		{
			"id": "api_key",
			"type": "promptString",
			"description": "your API key",
			"password": true
		},
		{
			"id": "CONTEXT7_API_KEY",
			"type": "promptString",
			"description": "API key for authentication",
			"password": true
		},
		{
			"id": "token",
			"type": "promptString",
			"description": "",
			"password": true
		},
		{
			"id": "project-ref",
			"type": "promptString",
			"description": "Supabase project reference ID",
			"password": false
		},
		{
			"id": "read-only",
			"type": "promptString",
			"description": "Enable read-only mode",
			"password": false
		},
		{
			"id": "features",
			"type": "promptString",
			"description": "Comma-separated list of features to enable",
			"password": false
		},
		{
			"id": "api-url",
			"type": "promptString",
			"description": "Custom API URL",
			"password": false
		},
		{
			"id": "SUPABASE_ACCESS_TOKEN",
			"type": "promptString",
			"description": "Personal access token for Supabase API",
			"password": true
		}
	]
}